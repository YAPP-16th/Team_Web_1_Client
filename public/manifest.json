{
  "name": "urLink",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "tabs", 
    "activeTab",
    "<all_urls>",
    "history",
    "storage"
  ],
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  }
}